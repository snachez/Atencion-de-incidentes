@model Atencion_de_incidentes.Models.TBMSMAETickets
@{
    Layout = null;
}
@using (Html.BeginForm("Reasignar", "Administrador", FormMethod.Post, new { @class = "form-horizontal form-label-left" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div id="result">@ViewBag.Error</div>
    @Html.HiddenFor(model => model.idTicket)
    <div class="form-group">
        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
            Técnico actual
        </label>
        <div class="col-md-9 col-sm-9 col-xs-12">
            <input type="text" name="title" class="form-control" value="@Html.DisplayFor(model => model.AspNetUsers1.NombreCompleto)" readonly="">
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-3 col-sm-3 col-xs-12" for="first-name">
            Asignar a técnico
        </label>
        <div class="col-md-9 col-sm-9 col-xs-12">
            @Html.DropDownList("IdUsuarioTec", null, htmlAttributes: new { @class = "form-control", @name = "IdUsuarioTec" })
        </div>
    </div>
    <div class="ln_solid"></div>
    <div class="form-group">
        <div class="col-md-9 col-sm-9 col-xs-12 col-md-offset-3">
            <button id="save_data" type="submit" class="btn btn-success">Guardar</button>
        </div>
    </div>
}
@section Scripts {
    <script src="~/Content/js/reasignarTicket_Administrador.js"></script>
}